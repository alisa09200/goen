<div class="contents row">
  <div class="container">
    <h3>授業PR動画を投稿する</h3>
    <%= form_with(model: @tweet, local: true) do |form| %>
      <%= form.label :２〜３分の動画 %>
      <%= form.file_field :video, :accept => 'video/*' %>
      <%= form.text_area :text, placeholder: "例）初級　文法　", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div